
Import('check_env')

env = check_env.Clone()

env.Prepend(LIBS = Split('''
                            gcomm
                            galerautils++
                            galerautils
                         '''))

env.Program(target = 'check_gcomm',
            source = Split('''
                              check_gcomm.cpp
                              check_trace.cpp
                              check_types.cpp
                              check_util.cpp
                              check_gmcast.cpp
                              check_evs2.cpp
                              check_pc.cpp
                           '''))
