# Default mysqld options
[mysqld]
innodb_flush_log_at_trx_commit=0
innodb_buffer_pool_size=420M
innodb_log_file_size=256M
innodb_doublewrite=0
#innodb_log_buffer_size=8M
#innodb_checksums=0
default-storage-engine=innodb

# This must be substituted by galera_ws_format/galera_replication_format
log_bin
binlog_format=ROW

# Galera options
# Group communication system handle
galera_gcs_address="dummy://"
# Group name
galera_gcs_group="my_cluster"
# How many threads will process writesets from other nodes
galera_slave_threads=1
#galera_dbug_option
galera_local_cache_size=16777216
galera_ws_persistency=0
galera_mark_commit_early=1
